	<script type="text/javascript">
	// GA events
	$(function() {
		$(".buy_now_paypal_btn").on("click", function(event) {
			ga("send", "event", "Listing", "Buy Open", null);
		});

		// listing flags
		$("#listing_flag_form").submit(function(event) {
			var reason = $('input[name=reason]:checked', '#listing_flag_form').val();
			ga("send", "event", "Listing", "Flagged", reason, 0, {transport: 'beacon'});
			return true;
		});

		// comment flags
		$("#comment_flag_form").submit(function(event) {
			var reason = $('input[name=reason]:checked', '#comment_flag_form').val();
			ga("send", "event", "Listing", "Comment Flagged", reason, 0, {transport: 'beacon'});
			ga("send", "event", "Listing Comments", "Flagged", reason, 0, {transport: 'beacon'});
			return true;
		});

		

	});

	</script>

	<script type="text/javascript">

		$('[data-toggle="tooltip"]').tooltip();

		$(function() {
			// Simple form validation for flag modals.
			$(".flag-modal button[type='submit']").click(function (e) {
				var form = $(this).parents(".modal-body");
				if(form.find("#reason > .active").length == 0){
					e.preventDefault();
					form.find("#reason").toggleClass("focus", true);
					setTimeout(function () {
						$("#reason").toggleClass("focus", false);
					}, 1000)
				}
			});

			$(".report-comment").click(function (e) {
				var form = $("#comment_flag_form");
				var action = form.attr("action").replace("LRML01410", this.getAttribute("data-uuid"));
				form.attr("action", action);
			});

			//client-side checking for image upload size
			$('#id_images').on('change',function(){
				if($(this).get(0).files.length > 0){ // only if a file is selected
					var totalSize = 0;
					var cumulativeSizeLimit = 50*1024*1024;
					var individualSizeLimit = 10*1024*1024;
					var overLimit = false;
					var fileList = $(this).get(0).files;
					for(var i = 0; i < fileList.length; i++){
						thisFileSize = fileList[i].size;
						totalSize += thisFileSize;
						if((thisFileSize > individualSizeLimit) || (totalSize > cumulativeSizeLimit)){
							overLimit = true;
						}
					}
					if(overLimit){
						$("#image_size_warning").show();
						$("#image_upload_form :input[type='submit']").prop('disabled', true);
					}else{
						$("#image_size_warning").hide();
						$("#image_upload_form :input[type='submit']").prop('disabled', false);
					}
				}
			});
		});

		function extend_listing_confirm(){
			if(confirm("Extend the listing's expiration date to 7 days from now?")) {
				window.location.replace("/listing/extend/LRML01410");
			}
		}
	</script>

	
