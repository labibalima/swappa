	<script type="text/javascript">
	$(function() {

		// update carrier links for specificity
		$(".carrier_pics a[data-slug]").each(function() {
			var href = $(this).attr("href");
			var slug = $(this).attr("data-slug");
			if (slug != "unlocked") {
				//href = href + "?type=phone&platform=ios";
				href = href + "/iphones";
				$(this).attr("href", href);
			}
		});

	});
	</script>

