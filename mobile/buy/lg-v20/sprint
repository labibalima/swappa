	$(function() {
		$('#product_price_modal').on('show.bs.modal', function() {
			$('div#pricing_chart').css('visibility', 'hidden');
		});
		$('#product_price_modal').on('shown.bs.modal', function() {
			$('div#pricing_chart')
				.css('visibility', 'initial')
				.highcharts().reflow();
		});
	});

</script>


