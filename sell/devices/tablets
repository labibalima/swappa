	<script>
	$(function() {

		// tooltips
		$(".has_tooltip").tooltip({
			"container": "body"
		});

		// for messages
		$(".messages .alert-error").addClass("alert-danger");

		
		$("input:not(.form-control)").each(function() {
			if (jQuery.inArray($(this).attr("type"), ["file", "checkbox", "radio"]) == -1) {
				$(this).addClass("form-control");
			}
		});

	});

	// GA events
	$(function() {

		// click to view a non-suggested listing
		$(".top_nav_item > a").on("click", function(event) {
			var text = $(this).text().trim();
			ga("send", "event", "Site Nav", "Top Click", text, 0, {transport: 'beacon'});
			//event.preventDefault();
		});
		$(".top_nav_item .dropdown-menu a").on("click", function(event) {
			var top_text = $(this).parents(".top_nav_item").children("a").text().trim();
			var drop_text = $(this).text().trim();
			var text = top_text + " > " + drop_text;
			ga("send", "event", "Site Nav", "Drop Click", text, 0, {transport: 'beacon'});
			//event.preventDefault();
		});

	});
	</script>
	

	<script type="text/javascript">
		function nav_platform(slug) {
			$("#list_form #id_platform").val(slug);
			$("#list_form").submit();
		}
		function nav_type(slug) {
			$("#list_form #id_type").val(slug);
			$("#list_form").submit();
		}
		function nav_search(q) {
			$("#list_form #id_search").val(q);
			$("#list_form").submit();
		}
		function clear_search() {
			nav_search("");
		}

		$(function() {

			
				$('#nav_type_').parent().hide();
			

			$("#list_form").submit(function() {
				// disable (not submit) empty fields
				$(this).children(':input[value=""]').attr("disabled", "disabled");
			});

			$("#sub_search_form").submit(function(e) {
				var q = $("#sub_search_form #id_search").val();
				nav_search(q);
				e.preventDefault();
			});

			// nav
			$("#nav_type_").addClass("active");
			$("#nav_platform_").addClass("active");

		});
	</script>

