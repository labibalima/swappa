	<script src="//ajax.microsoft.com/ajax/jquery.validate/1.9/jquery.validate.min.js" type="text/javascript"></script>
	<script>
		function hide_form() {
			var uri = window.location.toString();
			if (uri.indexOf("?") > 0) {
				$('#form_div').addClass('hidden');
				$('#link_div').removeClass('hidden');
				var clean_uri = uri.substring(0, uri.indexOf("?"));
				window.history.replaceState({}, document.title, clean_uri);
			}
		}

		function recaptchaCallback() {
		    $('#submitBtn').removeAttr('disabled');
		}

		$(function() {
			hide_form();

			$('.g-recaptcha').attr('data-callback', 'recaptchaCallback');

			$('#field_captcha > label').hide();

			$("#side_nav_help").addClass("active");
			$('#submitBtn').removeClass('form-control');

			$('#show_form').on('click', function(){
				$('#form_div').removeClass('hidden');
				$('#link_div').addClass('hidden');
			});

			$("#helpForm").validate({
				errorClass: "has-error",
				errorElement: "p",
				submitHandler: function(form) {
					form.submit();
				},
				highlight: function(element, errClass) {
					$(element).parents("div.form-group").addClass(errClass); //.removeClass("has-success");
				},
				unhighlight: function(element, errClass) {
					$(element).parents("div.form-group").removeClass(errClass); //.addClass("has-success");
				},
				errorPlacement: function(error, element) {
					error.appendTo(element.parents(".form-group").find(".errors"));
				},
				rules: {
					email: {
						required: true
					},
					subject: {
						required: true
					},
					message: {
						required: true

					},
					captcha: {
						required: true

					}
				}
			});

		});
	</script>





