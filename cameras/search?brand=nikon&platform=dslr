	<script type="text/javascript">
	
	function showFilterModal() {
		jQuery("#filters").detach().appendTo("#filter_modal .modal-body");
		$("#filter_modal").modal("show");
	}

	$(function () {
		$('#filter_modal').on('hide.bs.modal', function () {
			jQuery("#filters").detach().appendTo("#filter_container");
		});
	});
</script>

<div id="filter_modal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="filter_modal" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
				<h4 class="modal-title">
					Filter
				</h4>
			</div>
			<div class="modal-body">

			</div>
		</div>
	</div>
</div>






