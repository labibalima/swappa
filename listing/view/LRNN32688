	<script type="text/javascript">
		$(function() {
			$("#gallery_images").lightGallery({
				selector:"a.lightbox",
				download:false,
				speed:400
			});
			/* launch listing gallery from product image */
			$('#launch_gallery').click(function(){
				$('a.lightbox:first').trigger('click');
			});

			// detect screen size
			if ($(window).width() < 768) {
				$("#social_toolbar ").removeClass("btn-social");
			}
		});
	</script>

	<script type="text/javascript">
		function copyShareUrl(){
			  var copyText = document.getElementById("shareUrlInput");
			  copyText.select();
			  document.execCommand("Copy");
			  console.log("Copied the text: " + copyText.value);
		}
		/* load listing view with share modal triggered if ?share is passed as a URL parameter */
		$(document).ready(function(){
			var url = window.location.href,
				urlInfo = url.split('?'),
				urlParam = urlInfo[1];

			if (urlParam == 'share' ){
				jQuery("#shareModal").trigger('click');
				$('#shareModal').modal();
				$('.modal-title').text('Spread the word!');
				$("<p>Your listing has been approved, nice! Now it's time to spread the word. Sharing with family and friends is a great way to sell your listing fast</p>").insertAfter('.modal-title');
				//console.log("Share passed as a param ");
			}
		});
	</script>

