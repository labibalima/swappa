<script src="https://static.swappa.com/static/js/highcharts/highcharts.js" type="text/javascript"></script>
<script src="https://static.swappa.com/static/js/highcharts/modules/no-data-to-display.js" type="text/javascript"></script>

<script type="text/javascript">

function post_process(data){
	// override this function in the template to change chart options
	return data;
}

$(function(){
	$('div.pricing-chart').each(function(){
		var pricing_chart = $(this);
		var device_slug = $(this).data("device");
		var market_slug = $(this).data("market");

		var recent_prices_table = $(this).data("table-prices");
		var prices_url = "/prices/kwarg_device_slug/mk/json"
				.replace('kwarg_device_slug', device_slug)
				.replace('mk', market_slug);

		$.getJSON(prices_url, function(data) {
				data = post_process(data);
				pricing_chart.highcharts(data);
				if (recent_prices_table) {
					create_recent_prices(recent_prices_table, data['extra']['recent_prices'], data['extra']['currency']);
				}
			})
			.fail(function() {
				console.log( "error" );
			});
	});
});

function create_recent_prices(table_id, recent_prices, currency) {
	var html = "<th>Recent Sold Avg Price</th><th>Last Sold Price</th><th>Lowest Current Price</th></tr></thead>";

	// remove "price" word when table has small width
	if ($('#'+ table_id).width() < 500) {
		html = html.replace(/ Price/g, '');
	}

	if (recent_prices.length == 1) {
		html = "<thead><tr>" + html;
	} else {
		html = "<thead><tr><th style='text-align: left;'>Storage</th>" + html;
	}

	for(var i = 0; i < recent_prices.length; i++) {
		html += "<tr>";
		if (recent_prices.length > 1) {
			html += "<th>"+ recent_prices[i]['name'] +"</th>";
		}
		if (!recent_prices[i]['data']['recent_sold_price_avg']) {
			html += "<td>-</td>";
		} else {
			html += "<td>"+ currency + recent_prices[i]['data']['recent_sold_price_avg'] +"</td>";
		}
		if (!recent_prices[i]['data']['last_sold_price']) {
			html += "<td>-</td>";
		} else {
			html += "<td>"+ currency + recent_prices[i]['data']['last_sold_price'] +"</td>";
		}
		if (!recent_prices[i]['data']['current_low_price']) {
			html += "<td>-</td>";
		} else {
			html += "<td>"+ currency + recent_prices[i]['data']['current_low_price'] +"</td>";
		}
		html += "</tr>";
	}

	$("#"+ table_id).append(html);
}

</script>


		
		<script type="text/javascript">
		$(function() {
			$('#device_prices_modal').on('show.bs.modal', function() {
				$('div.pricing-chart').css('visibility', 'hidden');
			});
			$('#device_prices_modal').on('shown.bs.modal', function() {
				$('div.pricing-chart')
					.css('visibility', 'initial')
					.highcharts().reflow();
			});
		});
	
	</script>

	<script type="text/javascript">
		function copyShareUrl(){
			  var copyText = document.getElementById("shareUrlInput");
			  copyText.select();
			  document.execCommand("Copy");
			  //console.log("Copied the text: " + copyText.value);
		}
		/* load listing view with share modal triggered if ?share is passed as a URL parameter */
		$(document).ready(function(){
			var url = window.location.href,
				urlInfo = url.split('?'),
				urlParam = urlInfo[1];

			if (urlParam == 'share' ){
				jQuery("#shareModal").trigger('click');
				$('#shareModal').modal();
				$('.modal-title').text('Spread the word!');
				$("<p>Your listing has been approved, nice! Now it's time to spread the word. Sharing with family and friends is a great way to sell your listing fast</p>").insertAfter('.modal-title');
				//console.log("Share passed as a param ");
			}
		});


	</script>

	
