	// GA events
	$(function() {

		// click to view a non-suggested listing
		$(".top_nav_item > a").on("click", function(event) {
			var text = $(this).text().trim();
			ga("send", "event", "Site Nav", "Top Click", text, 0, {transport: 'beacon'});
			//event.preventDefault();
		});
		$(".top_nav_item .dropdown-menu a").on("click", function(event) {
			var top_text = $(this).parents(".top_nav_item").children("a").text().trim();
			var drop_text = $(this).text().trim();
			var text = top_text + " > " + drop_text;
			ga("send", "event", "Site Nav", "Drop Click", text, 0, {transport: 'beacon'});
			//event.preventDefault();
		});

	});
	</script>
	

