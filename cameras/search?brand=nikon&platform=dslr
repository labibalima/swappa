	<script type="text/javascript">
		$(function () {

			var products_auto = [];
			$.getJSON("/autocomplete/cameras.js", function(data) {
				products_auto = data;
			})
			.success(function() {

				$("#id_search").typeahead({
				  	source: products_auto,
					items: 15,
					minLength: 2,
					autoSelect: false,
					select: function () {
						var val = this.$menu.find('.active').data('value');
						this.$element.data('active', val);
						if (this.autoSelect || val) {
							var newVal = this.updater(val);
							// Updater can be set to any random functions via "options" parameter in constructor above.
							// Add null check for cases when updater returns void or undefined.
							if (!newVal) {
								newVal = '';
							}
							this.$element
								  .val(this.displayText(newVal) || newVal)
								  .text(this.displayText(newVal) || newVal)
								  .change();
							this.afterSelect(newVal);
						} else {
							$('#sub_search_form').submit();
						}
						return this.hide();
					},
					afterSelect: function (item) {
				  		window.location.href = "/mobile/buy/JS_PRODUCT_SLUG".replace('mobile', item.section).replace('JS_PRODUCT_SLUG', item.slug);
					},
					matcher: function (item) {
						var it = this.displayText(item);
						var keywords = this.query.toLowerCase().split(' ');

						for (var i = 0; i < keywords.length; i += 1) {
							if (keywords[i]) {
								if (it.toLowerCase().indexOf(keywords[i]) < 0) return false;
							}
						}
						return true
					},
					sorter: function (items) {
						var featured = [];
						var beginswith = [];
						var contains = [];
						var item;

						while ((item = items.shift())) {
							var it = this.displayText(item);
							if (item.is_featured) featured.push(item);
							else if (!it.toLowerCase().indexOf(this.query.toLowerCase())) beginswith.push(item);
							else contains.push(item);
						}

						return featured.concat(beginswith, contains);
					},
					highlighter: function (item) {
						var q = this.query.toLowerCase().split(" ");
						q= jQuery.grep(q, function(v){ return v !== ""; });
						//Surround any matches with the strong tag
						var regexp = new RegExp(q.join( '|' ),'gi');
						item = item.replace(regexp,function(matched){
							return "<strong>"+ matched +"</strong>";
						});
						return item;
					}
				});
			});

		})
	</script>


