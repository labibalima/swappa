	<script type="text/javascript">
	$(function () {

		$("a.btn_section").click(function(e) {
			var section = $(this).text().trim();
			if (section == "") section = $(this).next(".list-group").attr('data-section');
			ga('send', 'event', 'Sell', 'Section Head', section, 0, {transport: 'beacon'});
		});

		$("li.list-group-item > a").click(function(e) {
			// if a "section more" link then ignore so we don't double count
			if ($(this).hasClass("section_more")) return;
			// otherwise proceed
			var section = $(this).closest(".list-group").attr('data-section');
			var index = $(this).parent().index(); // $(this).closest(".list-group").index($(this).parent());
			ga('send', 'event', 'Sell', 'Section Item', section, index, {transport: 'beacon'});
		});

		$("a.section_more").click(function(e) {
			var section = $(this).closest(".list-group").attr('data-section');
