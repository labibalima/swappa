	<script>
		function showExpressShippingModal(yes_callback, no_callback) {
			// show modal
			$("#express_shipping_modal").modal('show');

			// button handlers
			if (yes_callback) {
				$("#express_shipping_yes_btn").unbind().click(yes_callback);
				$("#express_shipping_ok_btn").hide();
			} else {
				$("#express_shipping_ok_btn").show();
				$("#express_shipping_yes_btn").hide();
				$("#express_shipping_no_btn").hide();
			}
			if (no_callback) {
				$("#express_shipping_no_btn").unbind().click(no_callback);
			}
		}
	</script>

	<style>
		#express_shipping_modal ul li {
			padding: 3px 0px;
		}
	</style>


