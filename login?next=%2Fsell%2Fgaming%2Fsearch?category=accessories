	<script type="text/javascript">
	$(function() {
		//Trim when email field becomes unfocused. Purely for looks since it is also trimmed in the submit handler.
		$('#id_username').on('blur',function(e){
			e.target.value = $.trim(e.target.value);
		});
		//Change the validate email method to trim and then call the old validate email function with the result.
		var _oldemail = $.validator.methods.email;
		$.validator.methods.email = function (value, element) {
			return _oldemail.call(this, $.trim(value), element)
		};

		$("#formLogin").validate({
			errorClass: "has-error",
			errorElement: "p",
			submitHandler: function(form) {
				$(form).find('input').first().val('oScnhwKLyBjg5V9xfdw01bR0Fx78x7zR');
				//Trim email before submitting. Makes sure it gets trimmed for fringe cases where blur might not get called.
				$('#id_username').val(function(i,val){
					return $.trim(val);
				});
				
					grecaptcha.execute();
				
			},
			highlight: function(element, errClass) {
				$(element).parents("div.form-group").addClass(errClass); //.removeClass("has-success");
			},
			unhighlight: function(element, errClass) {
				$(element).parents("div.form-group").removeClass(errClass); //.addClass("has-success");
			},
			errorPlacement: function(error, element) {
				error.appendTo(element.parents(".form-group").find(".errors"));
			},
			rules: {
				username: {
					required: true,
					email: true
				},
				password: {
					required: true,
					minlength: 4
				}
			}
		});
	});

    //Analytics events for Login/Register
	function gaLoginEvents(label){
        ga('send', 'event', "Login and Signup", "click", label, 0, {transport: 'beacon'});
	}
	function onSubmit(token) {
		document.getElementById('formLogin').submit();
	}
	</script>

	
	

