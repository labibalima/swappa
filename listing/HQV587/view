	<script type="text/javascript">
	// comment submit button
	function disableSubmit(time) {
		$('#submit_button').attr("disabled", true);
		setTimeout('enableSubmit()', time);
	}
	function enableSubmit() {
		$('#submit_button').attr("disabled", false);
	}
	
	$(function() {
		$.validator.addMethod(
			"has_email",
			function(value, element, regexp) {
				var re = new RegExp(regexp);
				return this.optional(element) || !(re.test(value));
			}, "Please check your input."
		);
		$.validator.addMethod(
			"has_phone",
			function(value, element, regexp) {
				var re = new RegExp(regexp);
				return this.optional(element) || !(re.test(value));
			}, "Please check your input."
		);

		// comment form
		$("#formComment").validate({
			submitHandler: function(form) {
				form.submit();
				disableSubmit(5*1000);
			},
			errorPlacement: function(error, element) {
				error.appendTo("#comment_errors");
			},
			messages: {
				comment: {
					required: "The comment field is required.",
					maxlength: "The comment field is limited to 1000 characters.",
					has_email: "Personal contact information should not be shared. <a href='https://swappa.com/faq/answer/private_messaging' target='_blank'>See here for why.</a>",
					has_phone: "Personal contact information should not be shared. <a href='https://swappa.com/faq/answer/private_messaging' target='_blank'>See here for why.</a>"
				}
			},
			rules: {
				comment: {
					required: true,
					maxlength: 1000,
				
					has_email: "\\S+@\\S+\\.\\S+",
					has_phone: "\\d{3}[-\\.\\s]??\\d{3}[-\\.\\s]??\\d{4}|\\(\\d{3}\\)\\s*\\d{3}[-\\.\\s]??\\d{4}|\\d{3}[-\\.\\s]??\\d{4}"
				
				}
			}
		});
		
		// links in comments
		$(".comment .text a").attr("target", "_blank");
		
	});
	
	
	</script>
	
	
	
	
